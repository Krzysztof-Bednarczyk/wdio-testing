{
  "name": "wdio-testing",
  "version": "1.0.0",
  "description": "Example testing framework for WebdriverIO",
  "main": "index.js",
  "scripts": {
    "test": "npx wdio config/wdio.local.conf.js",
    "browserstack": "env-cmd -f .env.prod npx wdio config/wdio.browserstack.conf.js",
    "docker": "docker-compose up -d && npx wdio config/wdio.docker.conf.js && docker-compose down",
    "allure": "npx allure generate ./allure-results --clean && npx allure open",
    "pretty": "npx prettier --write ./test/**/*",
    "precommit": "npm run pretty && git add ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty",
      "pre-push": "npm test"
    }
  },
  "keywords": [
    "WebdriverIO",
    "Udemy",
    "Czech",
    "Rep"
  ],
  "author": "Krzysztof Bednarczyk",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/register": "^7.12.13",
    "@wdio/allure-reporter": "^7.0.3",
    "@wdio/browserstack-service": "^7.0.3",
    "@wdio/cli": "^7.0.3",
    "@wdio/local-runner": "^7.0.3",
    "@wdio/mocha-framework": "^7.0.3",
    "@wdio/selenium-standalone-service": "^7.0.3",
    "@wdio/spec-reporter": "^7.0.3",
    "@wdio/sync": "^7.0.3",
    "allure-commandline": "^2.13.8",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "husky": "^4.3.8",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "wdio-docker-service": "^3.1.0"
  }
}